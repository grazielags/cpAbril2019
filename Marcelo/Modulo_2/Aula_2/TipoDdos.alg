algoritmo "TipoDdos"
var
      nome: caractere
      idade: inteiro
      salario: real
      masculino: logico
Inicio
      nome <- "Maria"
      idade <- 18
      salario <- 100,00
      masculino <- falso
      escreval("Nome: ", nome)
      escreval("Idade: ", idade)
      escreval("Salario: ", salario)
      escreval("É masculino?: ", masculino)
fimalgoritmo
