algoritmo "Exercicio1"
var
   Valor1, Valor2: Inteiro
inicio
      Escreva(" Digite o primeiro valor: ")
      leia (Valor1)
      Escreva("Digite o segundo valor: ")
      Leia(Valor2)

      se Valor1 > Valor2 entao
      escreval("O Primeiro valor é maior do que Segundo valor")
      senao
      escreval("O Segundo valor é maior do que Primeiro valor")
      fimse
fimalgoritmo

