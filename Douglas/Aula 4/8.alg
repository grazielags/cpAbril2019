Algoritmo "7"
Var
   m, n, maior, menor, soma: inteiro


Inicio
      Escreval ("digite um valor para M")
      leia (m)
      maior<-m
           enquanto (m<>0) faca
      escreval ("digite um valor para N")
      leia (n)
              Se(n>-Maior)entao
              menor<-n
              fimse
      fimenquanto
      

Fimalgoritmo
