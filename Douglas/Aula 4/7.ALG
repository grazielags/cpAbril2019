Algoritmo "7"
Var
   n, tabuada, resultado: inteiro


Inicio
      Escreval ("digite um valor para n")
      leia (n)
      repita
            tabuada <- tabuada + 1
            resultado <- (tabuada*n)
            escreval (tabuada," x ",n, "= ", resultado)
      ate tabuada=10
      

Fimalgoritmo
