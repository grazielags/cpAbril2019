algoritmo "TiposDados"
var
   nome : caractere
   idade : inteiro
   salario : real
   masculino : logico
inicio
      nome <- "Maria"
      idade <- 18
      salario <- 100,00
      masculino <- falso
      escreval("Nome: ", nome)
      escreval("Idade: ", idade)
      escreval("Salario: ", salario)
      escreval("É masculino?: ", masculino)
fimalgoritmo

